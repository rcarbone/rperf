# Project libraries

# rlibc
${RLIBCLIB}:
	@echo "==================="
	@echo "=*= making ${RLIBCLIB} =*="
	@(cd ${RLIBCDIR} && make -s)
	@echo "==================="

# rtest
${RTESTLIB}:
	@echo "==================="
	@echo "=*= making ${RTESTLIB} =*="
	@(cd ${RTESTDIR} && make)
	@echo "==================="
